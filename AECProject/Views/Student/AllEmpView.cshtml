<!--Strong Type View-->
@model List<Student> 
<!--C# HTML-->
<!--@Model List<Student>-->
    

<table class="table table-hover table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Image</th>
    </tr>
    @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Image</td>
            <td>
                <img src="/Images/@item.Image" style="width:100px"/>
            </td>
        </tr>
    }
</table>
